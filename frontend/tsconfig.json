{
    "compilerOptions": {
        "sourceMap": true,
        "pretty": true,
        "module": "ES6",
        "target": "ES6",
        "jsx": "React"
    },
    "files": [
        "typings/index.d.ts",
        "src/index.tsx"
    ],
    "filesGlob": [
        "typings/index.d.ts",
        "src/**/*.ts",
        "src/**/*.tsx",
        "!node_modules/**"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "compileOnSave": false
}
